{
  "name": "zbrettonye/geetest",
  "description": "Geetest Package for Laravel",
  "keywords": [
    "Geetest",
    "Laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zbrettonye",
      "email": "monkeyblacktech97@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "illuminate/support": "^5.5 || ^6 || ^7",
    "illuminate/routing": "^5.5 || ^6 || ^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "ZBrettonYe\\Geetest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZBrettonYe\\Geetest\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
            "providers": [
                "ZBrettonYe\\Geetest\\GeetestServiceProvider"
            ],
            "aliases": {
                "Geetest": "ZBrettonYe\\Geetest\\Geetest"
            }
        }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
